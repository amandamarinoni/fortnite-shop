    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "@prisma/client": "^5.0.0"
  },
  "devDependencies": {
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0",
    "prisma": "^5.0.0",
    "ts-node": "^10.0.0",
    "eslint": "^8.0.0"
  }
}
